<table class="utable">
    <tr>
        <th>id</th>
        <th>username</th>
        <th>password</th>
        <th>groups</th>
    </tr>
    {{#each users as |user|}}
    <tr>
        <td>{{user.id}}</td>
        <td>{{user.name}}</td>
        <td>{{user.password}}</td>
        <td>{{user.groups}}</td>
    </tr>
    {{/each}}
</table>